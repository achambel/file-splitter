{
  "manifest_version": 2,

  "name": "__MSG_extName__",
  "description": "__MSG_extDescription__",
  "short_name": "__MSG_extShortName__",
  "default_locale": "pt_BR",
  "version": "1.1",
  "author": "Adriano Chambel M. de Lima <acmlima.softweb@gmail.com>",

  "permissions": ["tabs"],

  "browser_action": {
    "default_icon": "icon.png",
